{
  "manifest_version": 2,
  "name": "Multiple New Tabs",
  "version": "1.0.0",

  "description": "Easily switch between multiple new tab extensions",
  "homepage_url": "https://github.com/deadcoder0904/multiple-new-tab",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["management", "tabs", "storage"]
}
